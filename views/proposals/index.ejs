<!DOCTYPE html>
<html>
  <head>
    <%- include('./../partials/head') %>
    <%- include('./../partials/navbar') %>

    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h2>All Proposals</h2>
    <% proposals.forEach(function(proposal) { %>

      <h3><%= proposal.title %></h3>
      <p><%= proposal.content %></p>
      <p>Created by: <%= proposal.rep.firstname %><p>
      <!-- <h3>yay: <%= proposal.votes %> </h3> -->
      <p id="counter">Xxx</p>
      <button id="yay" class='<%= proposal._id%>'>Yay</button> <%= proposal.votes.length %> | <button id='nay'>Nay</button>
      <%= proposal.votes.length %>
    <% }) %>



  </body>
</html>
